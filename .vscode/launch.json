{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": ".NET MAUI",
            "type": "maui",
            "request": "launch",
            "preLaunchTask": "maui: Build"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net7.0/Soodookoo.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "C#: Soodookoo",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}\\Soodookoo.csproj",
            "launchConfigurationId": "TargetFramework=;Soodookoo"
        }
    ]
}